{
  "name": "roadsoft",
  "version": "2.0.0",
  "description": "Project to sync file to roadsoft server",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=develop electronmon .",
    "start": "electron .",
    "package": "electron-builder -p always",
    "build": "electron-packager . --asar --overwrite",
    "dist": "electron-builder --win --x64",
    "dist:mac": "npx electron-builder --mac"
  },
  "repository": "https://github.com/RS-Roadsoft/roadsoft-import-app",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "keywords": [
    "roadsoft",
    "sync-files"
  ],
  "author": "Usman",
  "license": "1.0",
  "devDependencies": {
    "cross-env": "^10.1.0",
    "electron": "^19.0.6",
    "electron-builder": "^23.6.0",
    "electron-packager": "^15.5.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electronmon": "^2.0.3"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "auto-launch": "^5.0.5",
    "axios": "^0.27.2",
    "electron-log": "^5.0.0-beta.16",
    "electron-updater": "^5.3.0",
    "knex": "^2.2.0",
    "sqlite3": "5.0.11"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "RS-Roadsoft",
        "repo": "roadsoft-import-app-public"
      }
    ]
  }
}
